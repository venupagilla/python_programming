    "import math\n",
    "def calc(x1,y1,x2,y2):\n",
    "    return math.sqrt((x2-x1)**2+(y2-y1)**2)\n",
    "\n",
    "x1=10\n",
    "y1=20\n",
    "x2=56\n",
    "y2=50\n",
    "print(f\"The distance between points ({x1},{y1}) and ({x2,y2}) is {int(calc(x1,y1,x2,y2))}\")"
   ]
  }
